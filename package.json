        "@sveltejs/kit": "^1.0.13",
        "@typescript-eslint/eslint-plugin": "^5.45.1",
        "@typescript-eslint/parser": "^5.45.1",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.28.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "postcss": "^8.4.23",
